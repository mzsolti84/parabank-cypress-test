{
  "name": "parabank-tests",
  "version": "1.0.0",
  "description": "Cypress test for a demo project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint cypress --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "cypress run",
    "report": "node ./cucumber-html-report.js",
    "cypress:open": "cypress open",
    "cypress:open:debug": "set DEBUG=cypress:electron,cypress-configuration,cypress-cucumber-preprocessor && npx cypress open --e2e --config-file=cypress.config.ts",
    "cypress:open:demo": "cypress open --config-file cypress-demo.config.ts",
    "cypress:run:demo": "cypress run --config-file cypress-demo.config.ts",
    "allure:result:clear": "del-cli --force allure-results allure-report cypress/screenshots",
    "allure:run:test": "npx cypress run --env allure=true",
    "allure:run:demo": "npx cypress run --config-file cypress-demo.config.ts --env allure=true",
    "allure:generate": "allure generate --clean -o allure-report",
    "allure:open": "allure open",
    "allure:all:demo": "npm run allure:result:clear & npm run allure:run:demo & npm run allure:generate && npm run allure:open",
    "allure:only:demo": "npm run allure:result:clear & npm run allure:run:demo -- --spec='**/a*-*.feature' & npm run allure:generate && npm run allure:open"
  },
  "dependencies": {
    "cypress-commands": "^2.0.1",
    "cypress-localstorage-commands": "^2.1.0",
    "cypress-tags": "0.3.0",
    "del-cli": "^5.0.0",
    "mocha": "^10.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "@testing-library/cypress": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "allure-commandline": "^2.23.0",
    "csv-parse": "^5.3.8",
    "cypress": "12.14",
    "cypress-cucumber-attach-screenshots-to-failed-steps": "^1.0.0",
    "cypress-promise": "^1.1.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-json-to-md": "^0.7.1",
    "mochawesome-merge": "^4.3.0",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "prettier": "2.7.0",
    "typescript": "^4.7.3"
  }
}
